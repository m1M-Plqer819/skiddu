--totorial script hub

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "m1M", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local oneTab = Window:MakeTab({
	Name = "script",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local PlayerTab = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Button!",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/m1M-Plqer819/skiddu/main/repl7/fly"))()

  	end    
})

Tab:AddButton({
	Name = "Button2",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/m1M-Plqer819/Skeyuirb/main/newbeta/loader.lua"))()

  	end    
})

PlayerTab:AddSlider({
	Name = "Slider",
	Min = 18,
	Max = 500,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "bananas",
	Callback = function(Value)
		game.Players.LocalPlayer.Character:SetAttribute("SpeedMultiplier",Value)
	end    
})

PlayerTab:AddSlider({
	Name = "Slider",
	Min = 18,
	Max = 500,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "bananas",
	Callback = function(Value)
		game.Players.LocalPlayer.Character:SetAttribute("JumpPower",Value)
	end    
})
